{
	"extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
	"plugins": ["prettier"],    
	"rules": {
		"prettier/prettier": "error",
		"no-unused-vars": "warn",
		"no-console": "off",
		"func-names": "off",
		"no-alert": "off",
		"object-shorthand": "off",
		"class-methods-use-this": "off"
	}
}